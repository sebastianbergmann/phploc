namespace PHPUnit\PharIo\Manifest;

class RequirementCollection implements \Countable, \IteratorAggregate
{
    /**
     * @var Requirement[]
     */
    private $requirements = [];
    public function add(\PHPUnit\PharIo\Manifest\Requirement $requirement)
    {
        $this->requirements[] = $requirement;
    }
    /**
     * @return Requirement[]
     */
    public function getRequirements()
    {
        return $this->requirements;
    }
    /**
     * @return int
     */
    public function count()
    {
        return \count($this->requirements);
    }
    /**
     * @return RequirementCollectionIterator
     */
    public function getIterator()
    {
        return new \PHPUnit\PharIo\Manifest\RequirementCollectionIterator($this);
    }
}
