/**
 * Reflection class for a {@}throws tag in a Docblock.
 */
final class Throws extends \PHPUnit\phpDocumentor\Reflection\DocBlock\Tags\TagWithType implements \PHPUnit\phpDocumentor\Reflection\DocBlock\Tags\Factory\StaticMethod
{
    public function __construct(\PHPUnit\phpDocumentor\Reflection\Type $type, ?\PHPUnit\phpDocumentor\Reflection\DocBlock\Description $description = null)
    {
        $this->name = 'throws';
        $this->type = $type;
        $this->description = $description;
    }
    public static function create(string $body, ?\PHPUnit\phpDocumentor\Reflection\TypeResolver $typeResolver = null, ?\PHPUnit\phpDocumentor\Reflection\DocBlock\DescriptionFactory $descriptionFactory = null, ?\PHPUnit\phpDocumentor\Reflection\Types\Context $context = null) : self
    {
        \PHPUnit\Webmozart\Assert\Assert::notNull($typeResolver);
        \PHPUnit\Webmozart\Assert\Assert::notNull($descriptionFactory);
        [$type, $description] = self::extractTypeFromBody($body);
        $type = $typeResolver->resolve($type, $context);
        $description = $descriptionFactory->create($description, $context);
        return new static($type, $description);
    }
    public function __toString() : string
    {
