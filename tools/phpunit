namespace PHPUnit\PharIo\Version;

class SpecificMajorVersionConstraint extends \PHPUnit\PharIo\Version\AbstractVersionConstraint
{
    /**
     * @var int
     */
    private $major = 0;
    /**
     * @param string $originalValue
     * @param int $major
     */
    public function __construct($originalValue, $major)
    {
        parent::__construct($originalValue);
        $this->major = $major;
    }
    /**
     * @param Version $version
     *
     * @return bool
     */
    public function complies(\PHPUnit\PharIo\Version\Version $version)
    {
        return $version->getMajor()->getValue() == $this->major;
    }
}
