namespace PHPUnit\TextUI\Configuration;

/**
 * @internal This class is not covered by the backward compatibility promise for PHPUnit
 */
final class Registry
{
    /**
     * @var self
     */
    private static $instance;
    /**
     * @var Configuration[]
     */
    private $configurations = [];
    public static function getInstance() : self
    {
        if (!self::$instance instanceof self) {
            self::$instance = new self();
        }
        return self::$instance;
    }
    private function __construct()
    {
    }
    private function __clone()
    {
    }
    public function get(string $filename) : \PHPUnit\TextUI\Configuration\Configuration
    {
        if (!isset($this->configurations[$filename])) {
            $this->configurations[$filename] = (new \PHPUnit\TextUI\Configuration\Loader())->load($filename);
        }
        return $this->configurations[$filename];
    }
}
