    /**
     * @internal This method is not covered by the backward compatibility promise for PHPUnit
     */
    public function hasOutput() : bool
    {
        if ($this->output === '') {
            return \false;
        }
        if ($this->hasExpectationOnOutput()) {
            return \false;
        }
        return \true;
    }
