    }
    public function hasExcludeGroups() : bool
    {
        return $this->excludeGroups !== null;
    }
    public function excludeGroups() : array
    {
        if ($this->excludeGroups === null) {
            throw new \PHPUnit\TextUI\Arguments\Exception();
        }
        return $this->excludeGroups;
    }
