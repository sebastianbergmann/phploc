use PHPUnit\Symfony\Polyfill\Ctype as p;
if (!\function_exists('ctype_alnum')) {
    function ctype_alnum($text)
    {
        return \PHPUnit\Symfony\Polyfill\Ctype\Ctype::ctype_alnum($text);
    }
